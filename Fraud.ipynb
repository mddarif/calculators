   "source": [
    "# Array Index Out of Bounds Example with Exception Handling\n",
    "\n",
    "arr = [10, 20, 30]\n",
    "\n",
    "try:\n",
    "    print(arr[3])  # Invalid index\n",
    "except IndexError:\n",
    "    print(\"Index out of bound error occurred\")\n",
    "\n",
    "print(\"Program continues normally\")"
   ]
  }
